{
  "name": "LGU Blackboard downloader",
  "icons": {
    "16": "./cuhk.png",
    "32": "./cuhk-1.png",
    "48": "./cuhk-2.png",
    "128": "./cuhk-3.png"
  },
  "description": "An extension to download all you need in the LGU (aka CUHKSZ) Blackboard",
